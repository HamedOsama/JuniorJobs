
generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch", "fullTextIndex"]
}

datasource db {
  provider = "mysql"
  url      = env("MAIN_DATABASE_URL")
  relationMode = "prisma"
}


model Job {
  id       String    @id @unique @default(cuid())
  company  String
  title    String
  link     String
  email    String
  type     String
  deadline String
  logo     String
  skills   String

  @@fulltext(fields: [title])
  @@fulltext(fields: [skills])
}
model Lister{
  id      String @id @unique @default(cuid())
  name    String
  token   String
}

